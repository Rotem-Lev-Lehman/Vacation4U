<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.SearchPageView">
   <children>
      <TextField fx:id="searchTextField" layoutX="40.0" layoutY="62.0" prefHeight="25.0" prefWidth="198.0" />
      <TableView fx:id="searchResultTable" layoutX="2.0" layoutY="124.0" prefHeight="230.0" prefWidth="592.0">
        <columns>
          <TableColumn fx:id="usernameColumn" prefWidth="75.0" text="Username" />
            <TableColumn fx:id="passwordColumn" prefWidth="75.0" text="Password" />
          <TableColumn fx:id="firstNameColumn" prefWidth="75.0" text="First Name" />
            <TableColumn fx:id="lastNameColumn" prefWidth="75.0" text="Last Name" />
            <TableColumn fx:id="cityColumn" prefWidth="75.0" text="City" />
            <TableColumn fx:id="birthDateColumn" prefWidth="75.0" text="Birth Date" />
        </columns>
      </TableView>
      <ImageView fx:id="magViewBox" fitHeight="25.0" fitWidth="29.0" layoutX="252.0" layoutY="62.0" onMouseClicked="#searchButtonClicked" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
